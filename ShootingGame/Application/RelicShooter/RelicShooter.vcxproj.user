    <LocalDebuggerEnvironment>PATH=%PATH%;$(ProjectDir)/Image</LocalDebuggerEnvironment>
  </PropertyGroup>
  <PropertyGroup>
    <ShowAllFiles>false</ShowAllFiles>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <LocalDebuggerWorkingDirectory>$(ProjectDir)/Image</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerEnvironment>PATH=%PATH%;$(ProjectDir)/Image/x64</LocalDebuggerEnvironment>
  </PropertyGroup>
